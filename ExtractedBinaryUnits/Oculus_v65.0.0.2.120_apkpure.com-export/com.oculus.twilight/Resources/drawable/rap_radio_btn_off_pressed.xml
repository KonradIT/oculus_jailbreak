<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="1">
            <solid android:color="@color/rap_radio_pressed"/>
            <size android:height="@dimen/rap_radio_size" android:width="@dimen/rap_radio_size"/>
        </shape>
    </item>
    <item android:left="@dimen/rap_radio_ring_inset" android:top="@dimen/rap_radio_ring_inset" android:right="@dimen/rap_radio_ring_inset" android:bottom="@dimen/rap_radio_ring_inset">
        <shape android:shape="1">
            <solid android:color="@android:color/transparent"/>
            <stroke android:width="@dimen/rap_radio_stroke_size" android:color="@color/rap_radio_ring_color"/>
        </shape>
    </item>
</layer-list>
