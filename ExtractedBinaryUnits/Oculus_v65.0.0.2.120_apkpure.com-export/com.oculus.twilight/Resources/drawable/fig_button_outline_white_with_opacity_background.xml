<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" android:variablePadding="false">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/fig_button_radius"/>
            <solid android:color="@color/fds_white_alpha20"/>
            <stroke android:width="@dimen/fig_button_stroke_width" android:color="?com.oculus.twilight:attr/fds_usage_always_white"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="@dimen/fig_button_radius"/>
            <solid android:color="@color/fds_black_alpha20"/>
            <stroke android:width="@dimen/fig_button_stroke_width" android:color="?com.oculus.twilight:attr/sutro_usage_button_border"/>
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="@dimen/fig_button_radius"/>
            <solid android:color="@color/fds_black_alpha20"/>
            <stroke android:width="@dimen/fig_button_stroke_width" android:color="?com.oculus.twilight:attr/fds_usage_always_white"/>
        </shape>
    </item>
</selector>
