<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_first="true" android:state_last="true" android:state_pressed="true">
        <shape>
            <solid android:color="@color/fbui_popover_list_view_selector"/>
            <corners android:radius="@dimen/fbui_popover_list_view_corner_radius"/>
        </shape>
    </item>
    <item android:state_first="true" android:state_pressed="true">
        <shape>
            <solid android:color="@color/fbui_popover_list_view_selector"/>
            <corners android:topLeftRadius="@dimen/fbui_popover_list_view_corner_radius" android:topRightRadius="@dimen/fbui_popover_list_view_corner_radius"/>
        </shape>
    </item>
    <item android:state_last="true" android:state_pressed="true">
        <shape>
            <solid android:color="@color/fbui_popover_list_view_selector"/>
            <corners android:bottomLeftRadius="@dimen/fbui_popover_list_view_corner_radius" android:bottomRightRadius="@dimen/fbui_popover_list_view_corner_radius"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/fbui_popover_list_view_selector"/>
        </shape>
    </item>
</selector>
